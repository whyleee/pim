# http://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
#end_of_line = lf # doesn't work in vs
insert_final_newline = true
trim_trailing_whitespace = true

[*.cs]
indent_size = 4

[*.sln]
indent_style = tab

[*.md]
insert_final_newline = false
trim_trailing_whitespace = false
